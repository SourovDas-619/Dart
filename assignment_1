List<String> phoneNumbers = [
      '+88',
      '01768131685',
      '01768171985',
      '01768111286',
      '01768131685'
    ];
    
    String number1 = phoneNumbers[0] + phoneNumbers[1];
    String number2 = phoneNumbers[0] + phoneNumbers[2];
    String number3 = phoneNumbers[0] + phoneNumbers[3];
    String number4 = phoneNumbers[0] + phoneNumbers[4];
    print(number1 + ',' + number2 + ',' + number3 + ',' +number4);
